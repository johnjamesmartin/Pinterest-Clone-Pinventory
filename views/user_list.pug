extends layout

block content
  h1="Users"
  table.table.table-striped
    thead
      tr
        th="Username"
        th="Avatar"
        th="Github ID"
        th="Pinned"
        th="Saved"
    tbody
      each user in user_list
        tr
          td=user.username
          td
            img(src=user.avatar style="width: 30px;")
          td=user.githubId
          td="--"
          td="--"